{
  "name": "test-blog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run lint && nodemon src/server/launcher/index.js -x babel-node",
    "build-server": "babel src/server -d dist_server",
    "serve": "npm run build-server && node dist_server/launcher/index",
    "ssr-build": "rimraf dist_ssr && npm run ssr-build:client && npm run ssr-build:server",
    "ssr-build:client": "cross-env NODE_ENV=production webpack --config src/ssr-client/build/webpack.client.config.babel.js --progress --hide-modules",
    "ssr-build:server": "cross-env NODE_ENV=production webpack --config src/ssr-client/build/webpack.server.config.babel.js --progress --hide-modules",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "lru-cache": "^5.1.1",
    "morgan": "~1.9.1",
    "mysql": "^2.17.1",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vue-server-renderer": "^2.6.10",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.1",
    "eslint": "5.16.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mini-css-extract-plugin": "^0.8.0",
    "nodemon": "^1.19.1",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
